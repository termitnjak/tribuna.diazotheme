<?xml version="1.0" encoding="UTF-8"?>
<!--Rules for the articles view with content carousel-->
<rules
    xmlns="http://namespaces.plone.org/diazo"
    xmlns:css="http://namespaces.plone.org/diazo/css"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
    xmlns:xi="http://www.w3.org/2001/XInclude">

  <theme href="tribuna_mainpage.html"/>
  <replace css:theme-children="#header" css:content-children="#header" />
  <replace css:theme="#main" css:content="#main" />
  <replace css:theme="#close-articles" css:content="#close-articles" />
  <after css:theme="#main" css:content="#article-navigation" />

</rules>
